<!DOCTYPE html>
<header>
<h1>TinyApp</h1>
        <% if(user){ %>
<h1><%= user %></h1>
    <form method="POST" action="/logout">
        <input type="submit" value="Logout"/>
    </form>
    <% } else { %>  
<h2>Register here</h2>
   <form method="POST" action="/login">
    <input name="username" />
    <input type="submit" value="Login"/>
    </form>
<% } %>
</header>
<h2> TinyApp </h2>
  <p> <a href="/urls/new">Create new tiny URL </a> | <a href="/urls">TinyURLS</a> </p></p>
<% if (!user) { %>
  <p> <a href="/register">Register</a> | <a href="/login">Login</a> </p>
<% } else { %>
  <Table > <tbody><tr><td>Logged in as: <%= user.email %>! &nbsp;</td><td><form method="GET" action="/logout">
        <button type="submit" >Logout</button>
  </form></td></tr><tbody></Table>
  <p></p>
<% } %>

